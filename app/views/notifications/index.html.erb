<h1>Listing notifications</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Other</th>
      <th>Content</th>
      <th>Notification url</th>
      <th>Image url</th>
      <th>B read</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @notifications.each do |notification| %>
      <tr>
        <td><%= notification.user_id %></td>
        <td><%= notification.other_id %></td>
        <td><%= notification.content %></td>
        <td><%= notification.notification_url %></td>
        <td><%= notification.image_url %></td>
        <td><%= notification.b_read %></td>
        <td><%= link_to 'Show', notification %></td>
        <td><%= link_to 'Edit', edit_notification_path(notification) %></td>
        <td><%= link_to 'Destroy', notification, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Notification', new_notification_path %>
